        <script>
            // Create cookie
            function setCookie(cname, cvalue, exdays) {
                const d = new Date();
                d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                let expires = "expires=" + d.toUTCString();
                document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
            }

            // Delete cookie
            function deleteCookie(cname) {
                const d = new Date();
                d.setTime(d.getTime() + (24 * 60 * 60 * 1000));
                let expires = "expires=" + d.toUTCString();
                document.cookie = cname + "=;" + expires + ";path=/";
            }

            // Read cookie
            function getCookie(cname) {
                let name = cname + "=";
                let decodedCookie = decodeURIComponent(document.cookie);
                let ca = decodedCookie.split(';');
                for (let i = 0; i < ca.length; i++) {
                    let c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }

            function getval(callback) {
                $.getJSON('https://jsonip.com?callback=?', function (data) {
                    callback(data.ip);
                });
            }
            // Set cookie consent
            function acceptCookieConsent() {
                localStorage.setItem("Response", true);
                localStorage.setItem("cookies", true);
                deleteCookie('user_cookie_consent');
                setCookie('user_cookie_consent', 1, 30);
                var today = new Date();
                var date = today.getFullYear() + '-' + (today.getMonth() + 1) + '-' + today.getDate();
                setCookie('user_date', date);
                var location = window.location.href
                setCookie('user_track', `${location} @ ${date}`);
                getval(function (value) {
                    setCookie('user_ip', value);
                    var address = localStorage.getItem("cookies");
                    setCookie('user_ip', value);
                });
                document.querySelector("#cookiediv").style.transform = "translateY(130%)"
                document.querySelector("#FooterSec").style.paddingBottom = "0rem"
                /* document.getElementById("cookiediv").style.display = "none";*/
                document.querySelector("#back-to-top").style.margin = "0rem 0rem 0rem 0rem"
            }
            // decline cokie
            function declineCookieConsent() {
                localStorage.setItem("Response", true);
                localStorage.setItem("cookies", false);
                //$('#exampleModal').modal('hide');
                document.querySelector("#cookiediv").style.transform = "translateY(130%)"
                document.querySelector("#FooterSec").style.paddingBottom = "0rem"
                /*document.getElementById("cookiediv").style.display = "none";*/
                document.querySelector("#back-to-top").style.margin = "0rem 0rem 0rem 0rem"
            }
            // onload function
            $(document).ready(function () {

                let cookiediv = document.getElementById("cookiediv");
                let Response = localStorage.getItem("Response");
                let backdrop = document.querySelector(".cookiePolicyBackgroundDrop");
                backdrop.style.display = "none";
                if (!Response) {
                    localStorage.setItem("Response", false);
                }
                let afterLoadResponse = localStorage.getItem("Response");
                if (afterLoadResponse == "false") {
                    document.querySelector("#cookiediv").style.display = "flex";
                    document.querySelector("#FooterSec").style.paddingBottom = "4%";
                }
                else {
                    document.getElementById("cookiediv").style.display = "none";
                    document.querySelector("#FooterSec").style.paddingBottom = "0rem";
                }
                callAfterReady();
            })
            function callAfterReady() {
                setTimeout(function () {
                    let Response = localStorage.getItem("Response");
                    if (Response == "false") {
                        //$('#divcooki').modal('show');
                        document.querySelector("#cookiediv").style.transform = "translateY(0%)"
                        document.getElementById("cookiediv").style.display = "flex";

                        let cookie = document.querySelector("#cookiediv");
                        if (cookie.style.display != "none") {
                            document.querySelector("#back-to-top").style.margin = "0rem 0rem 8% 0rem"
                        }
                    }
                }, 2000)
            }

        </script>
   

   
