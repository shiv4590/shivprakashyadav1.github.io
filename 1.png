    <script type="text/javascript">
        function ClearPtrDetForm() {
            $(".Regerror").val("");
            $(".form-control").val("");
            AutoPopulateCtry();
        }

        function OnlineSiSection(type) {
            if (type == "remove") {
                $("#OnlineSiSection").hide();
            }
            else if (type == "Noremove") {
                $("#OnlineSiSection").show();
            }
        }

        function FocusPoint(type) {
            //alert(type);
            $("#cartFunction").show();
            $("#OPHBlockPaymentContent").hide();
            if (type == "ProceedToCartNormal" || type == "ProceedToCartSI" || type == "ProceedToCartOnlyDonation") {
                $("#CartModalPopupLink").click();
                $(".CartFormSec").show(); $(".SICheckBoxDet").show(); $(".NormalPGDet").show(); $(".OnlineSIDet").hide(); $(".SIOTD_CartContent").hide(); $(".OTDSI_CartContent").hide();
                $('input[name="AutoSISelect"]').prop("checked", false);
                if (type == "ProceedToCartNormal") {
                    $("[id$=BtnUnCheckedSI]").trigger("click");
                    $(".ddlPeriod").removeAttr("disabled");
                    return false;
                }
                else if (type == "ProceedToCartSI") {
                    $(".NormalPGDet").hide();
                    $(".OnlineSIDet").show();
                    $("#AutoSISelect").show();
                    $('input[name="AutoSISelect"]').prop("checked", true);
                    $("[id$=BtnSICardDetails]").trigger("click");
                    return false;
                }
                else if (type == "ProceedToCartOnlyDonation") {
                    $(".SICheckBoxDet").hide();
                    $("[id$=BtnUnCheckedSI]").trigger("click");
                    $(".ddlPeriod").removeAttr("disabled");
                    return false;
                }
            }
            else if (type == "ProceedToCartNoItem") {
                $(".SICheckBoxDet").hide();
                $("#CartModalPopupLink").click();
                $("[id$=BtnUnCheckedSI]").trigger("click");
                $(".CartFormSec").show();
                return false;
            } else if (type == "RegError") {
                $("#RegisterModalPopupLink").trigger("click");
                AutoPopulateCtry();
                //  $("[id$=Btn_Update]").trigger("click");
                return false;
            } else if (type == "PasswordError") {
                $("#SignInModalPopupLink").trigger("click");
                return false;
            } else if (type == "BlockFCRAPayment") {
                $("#CartModalPopupLink").trigger("click");
                $("#cartFunction").hide();
                $("#OPHBlockPaymentContent").show();
                return false;
            } else {
                alert("Something went wrong. Please try again later.");
                return false;
            }
            return true;
        }
        function PopulateFormValues(Nat, Ctry, State, DDlTitle) {
            $(".Ddl_Country").val(Ctry);
            $(".Ddl_State").val(State);
            $(".Hdn_Nationality").val(Nat);
            initCountry(Ctry, State);
            $(".Ddl_Title").val(DDlTitle);
            $(".Ddl_Nationality").val(Nat);
            $(".Ddl_Nationality").attr("disabled", "true");
        }
        function SMSFunctionality(type) {
            $("#rfvtxtEmail").html(""); $("#rfvtxtEmail").hide(); $(".OTPMessage").html(""); $(".OTPMessage").hide();
            if ($(".OTPMessage").hasClass("Success")) $(".OTPMessage").removeClass("Success");
            if (type == "NoMobileReg") {
                $("#rfvtxtEmail").html("The Email ID/ Mobile number is not registered with us."); $("#rfvtxtEmail").show();
            } else if (type == "SMSSentSuccess") {
                $(".OTPMessage").html("SMS sent successfully."); $(".OTPMessage").show(); $(".OTPMessage").addClass("Success");
            } else if (type == "ProblemWithSMS") {
                $(".OTPMessage").html("Problem with SMS service. Please try again later."); $(".OTPMessage").show();
            } else if (type == "OTPExpired") {
                $(".OTPMessage").html("Your OTP is Expired"); $(".OTPMessage").show();
            } else if (type == "InvalidOTP") {
                $(".OTPMessage").html("Invalid Password/OTP"); $(".OTPMessage").show();
            } else if (type == "InvalidPasswordOTP") {
                $("#rfvtxtEmail").html("Invalid Password/OTP"); $("#rfvtxtEmail").show();
            }
        }
    </script>
